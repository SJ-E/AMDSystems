       function submit_val(value){
  //  $('#fullpage-loader').show();
           // var value=[];
            //var karthi=$(this).val();//
            if(value != ''){
          // var karthi=$(this).val();
          $.post("http://amdsystems.in/billing/products/ajax/process_product_selections", {
                product_ids: value
            }, function (data) {
                                items = JSON.parse(data);

                for (var key in items) {
                    // Set default tax rate id if empty
                    if (!items[key].tax_rate_id) items[key].tax_rate_id = 0;

                    //if ($('#item_table tbody:last input[name=item_hidden_name]').val() !== '') {
                        $('#new_row').clone().appendTo('#item_table').removeAttr('id').addClass('item').hide();
                    //}

                    var last_item_row = $('#item_table tbody:last');

                    last_item_row.find('input[name=item_hidden_name]').val(items[key].product_name);
                    last_item_row.find('input[name=item_description]').val(items[key].product_description);
                    last_item_row.find('input[name=item_price]').val(items[key].product_price);
                    last_item_row.find('input[name=item_quantity]').val('1');
                    last_item_row.find('select[name=item_tax_rate_id]').val(items[key].tax_rate_id);
                    last_item_row.find('input[name=item_product_id]').val(items[key].product_id);

                    $('#modal-choose-items').modal('hide');
                }
               // $('#fullpage-loader').show();
                 var items = [];
            var item_order = 1;

            $('table tbody.item').each(function (key) {


              //  if(key !=0 || $('table tbody.item').length == 1){
                var row = {};
                $(this).find('input,select,textarea').each(function () {
                    if ($(this).is(':checkbox')) {
                        row[$(this).attr('name')] = $(this).is(':checked');
                    } else {

                        if($(this).attr('name') != undefined){
                            if($(this).attr('name') == 'item_hidden_name')
                                row['item_name'] = $(this).val();
                                else

                        row[$(this).attr('name')] = $(this).val();
                    }
                    }
                });
                row['item_order'] = item_order;
                item_order++;
                items.push(row);
                console.log(items);
           // }
console.log(items);
            });
            $.post("http://amdsystems.in/billing/invoices/ajax/save", {
                    invoice_id: 87,
                    invoice_number: $('#invoice_number').val(),
                    invoice_date_created: $('#invoice_date_created').val(),
                    invoice_date_due: $('#invoice_date_due').val(),
                    invoice_status_id: $('#invoice_status_id').val(),
                    invoice_password: $('#invoice_password').val(),
                    items: JSON.stringify(items),
                    invoice_discount_amount: $('#invoice_discount_amount').val(),
                    invoice_discount_percent: $('#invoice_discount_percent').val(),
                    invoice_terms: $('#invoice_terms').val(),
                    custom: $('input[name^=custom]').serializeArray(),
                    payment_method: $('#payment_method').val()
                },
                function (data) {
                                        var response = JSON.parse(data);
                  // $('#btn_generate_pdf').trigger('click');
                    if (response.success == '1') {
                        window.location = "http://amdsystems.in/billing/invoices/view/" + 87;
                    }
                    else {
                        $('#fullpage-loader').hide();
                        $('.control-group').removeClass('has-error');
                        $('div.alert[class*="alert-"]').remove();
                        var resp_errors = response.validation_errors,
                            all_resp_errors = '';
                        for (var key in resp_errors) {
                            $('#' + key).parent().addClass('has-error');
                            all_resp_errors += resp_errors[key];
                        }
                        $('#invoice_form').prepend('<div class="alert alert-danger">' + all_resp_errors + '</div>');
                    }
                });

                        //$('#fullpage-loader').hide();
            });
      }

